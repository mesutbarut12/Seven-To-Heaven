<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/ua_ua_name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="8dp"
        android:text="Name"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/ua_ua_name1"
        app:layout_constraintEnd_toStartOf="@+id/ua_ua_datum"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ua_ua_datum"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Datum"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ua_ua_name"
        app:layout_constraintEnd_toStartOf="@+id/ua_ua_prozent"
        app:layout_constraintStart_toEndOf="@+id/ua_ua_name"
        app:layout_constraintTop_toTopOf="@+id/ua_ua_name" />

    <TextView
        android:id="@+id/ua_ua_prozent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Prozent"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ua_ua_datum"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ua_ua_datum"
        app:layout_constraintTop_toTopOf="@+id/ua_ua_datum" />

    <TextView
        android:id="@+id/ua_ua_name1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="@+id/ua_ua_name"
        app:layout_constraintStart_toStartOf="@+id/ua_ua_name"
        app:layout_constraintTop_toBottomOf="@+id/ua_ua_name" />

    <TextView
        android:id="@+id/ua_ua_datum1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ua_ua_name1"
        app:layout_constraintEnd_toEndOf="@+id/ua_ua_datum"
        app:layout_constraintStart_toEndOf="@+id/ua_ua_name"
        app:layout_constraintTop_toTopOf="@+id/ua_ua_name1" />

    <TextView
        android:id="@+id/ua_ua_prozent1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/ua_ua_datum1"
        app:layout_constraintEnd_toEndOf="@+id/ua_ua_prozent"
        app:layout_constraintStart_toEndOf="@+id/ua_ua_datum"
        app:layout_constraintTop_toTopOf="@+id/ua_ua_datum1" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Beschreibung"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/ua_ua_beschreibung"
        app:layout_constraintEnd_toStartOf="@+id/imageView3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ua_ua_name1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/textView15"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_arrow_drop_down" />

    <TextView
        android:id="@+id/ua_ua_beschreibung"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:inputType="textMultiLine"
        android:text="TextView"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/textView15"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />
</androidx.constraintlayout.widget.ConstraintLayout>